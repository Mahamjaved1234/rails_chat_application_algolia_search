<p style="color: green"><%= notice %></p>
<div>
  <%= link_to "Home", '/addresses/' %><br>
   <%= link_to "Logout", "/" %>
</div>
<h2>SubCategory</h2>
<%= render @category %>
<div>
  <%= link_to "Back to categories", categories_path %>
</div>
<hr>
<h3><%= @category.name %> subcategories:</h3> 

<table>
  <tr>
    <th>Name</th>
    <th></th>
  </tr>
<% @subcategory.each do |subcategory| %>
  <tr>
    <td><%= subcategory.name %></td>
    <td><%= link_to "Select ", subcategory %></td>
    
  </tr>
<% end %>



